{
   "id":"nginx",
   "kind": "Pod",
   "apiVersion": "v1",
   "metadata": {
      "name": "nginx",
      "labels": {
         "name": "nginx"
      }
   },
   "spec": {
      "containers": [
         {
            "name": "nginx",
            "image": "nginx",
            "ports": [
               {
                  "hostPort": 85,
                  "containerPort": 80
               }
            ]
         }
      ]
   }
}